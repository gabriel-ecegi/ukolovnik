{
  "name": "my-dock-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "pnpm dlx @biomejs/biome lint .",
    "build:dev": "env-cmd -f .env.development pnpm run build",
    "build:test": "env-cmd -f .env.test pnpm run build",
    "build:prod": "env-cmd -f .env.prod pnpm run build",
    "preview": "vite preview --port 5001 --strictPort",
    "generate-types": "pnpm dlx rest2ts -s https://localhost:7122/swagger/v1/swagger.json -t ./src/Infrastructure/Api -v import.meta.env.VITE_API_URL --nullstr 1 --cookies 1"
  },
  "dependencies": {
    "@easepick/bundle": "^1.2.1",
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-date-pickers": "^6.19.4",
    "@originjs/vite-plugin-federation": "^1.3.4",
    "@tanstack/react-query": "^5.20.5",
    "@tanstack/react-query-devtools": "^5.20.5",
    "bignumber.js": "^9.1.2",
    "date-fns": "2.30.0",
    "env-cmd": "^10.1.0",
    "i18next": "^23.8.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easepick": "^1.0.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.0.5",
    "react-imask": "^7.4.0",
    "react-router-dom": "^6.22.0",
    "ts-object-path": "^0.1.2",
    "vite-plugin-biome": "^1.0.8",
    "vite-plugin-checker": "^0.6.4",
    "zod": "^3.22.4",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
